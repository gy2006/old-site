- bughd_token = htmlWebpackPlugin.options.bughd_token
doctype html
html
  block language
    - language = htmlWebpackPlugin.options.language
  head
    meta(charset='utf-8')
    meta(property="wb:webmaster", content="977ca36fa31b75c1")
    meta(http-equiv="last-modified" content="<%= htmlWebpackPlugin.options.last_modify %>")
    meta(name="viewport", content="width=device-width,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no")

    block title
      title flow.ci | Hosted Continuous Integration and Delivery Service
    script(type="text/javascript", src="https://dn-bughd-web.qbox.me/bughd.min.js", crossOrigin="anonymous")
    script(type="text/javascript").
      window.bughd = window.bughd || function(){};
      bughd("create",{key: #{bughd_token}});
    script.
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-43616652-7', 'auto');
      ga('send', 'pageview');
  body
    include ../includes/beta_bar
    include ../includes/navbar
    block content

